#!/usr/bin/gnuplot

year="2012a"

#set nokey
set xdata time
set timefmt "%d.%m %H:%M"
set title "температура"
set style data lines

plot ["10.03":"27.03"] [-40:10]\
  "../dat/30547_2013_"   using 1:3  lc 4 title "в Курумкане",\
  "muj13.dat"  using 1:3 with linespoints ls 7 ps 1.5 lc 3 title "в походе",\
-10 lc 0 title "",\
-20 lc 0 title "",\
-30 lc 0 title "",\
-40 lc 0 title "",\
0 lc 0 title ""

pause -1

set terminal png size 1024,768
#set terminal fig metric
set output "muj1.png"
replot

