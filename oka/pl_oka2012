#!/usr/bin/gnuplot

#set nokey
set xdata time
set timefmt "%Y.%m.%d"
set title "Уровень Оки 2012"
set style data linespoints
plot \
 "oka2012a.txt" using 1:($2-71)  title "Белёв",\
 "oka2012a.txt" using 1:($3+138) title "Калуга",\
 "oka2012a.txt" using 1:($4+48)  title "Алексин",\
 "oka2012a.txt" using 1:($5+142) title "Серпухов",\
 "oka2012a.txt" using 1:($6+229) title "Кашира",\
 "oka2012a.txt" using 1:($7+86)  title "Рязань",\
0 title ""
pause -1